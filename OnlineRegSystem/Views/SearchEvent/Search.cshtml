@{
    ViewBag.Title = "Search";
}

<h2> Search Results</h2>

<table class="table">
    <tr>
        <th> Event Name</th>
        <th> Event Description</th>
        <th> Event Location</th>
        <th> Event Date</th>
        <th> Event TicketFee</th>
    </tr>

    @foreach (var i in Model)
    {
    <tr>
        <td> @i.EventName </td>
        <td> @i.Description </td>
        <td> @i.Location </td>
        <td> @i.Date </td>
        <td> @i.TicketFee </td>
    </tr>
    }
</table>
<div style=" width: 200px">
    <input class="w3-button w3-red" type='button' onclick='history.go(-1);' value='Go Back' />
</div>